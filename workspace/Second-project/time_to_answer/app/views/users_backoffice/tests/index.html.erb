
<p><%= @all_tests_grade %></p>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Prova</th>
      <th scope="col">Respondida em</th>
      <th scope="col">Numero de quest√µes</th>
      <th scope="col">Nota</th>
    </tr>
  </thead>
  <tbody>
    <% @tests.each do |test| %>
      <th scope="row"> <%= test.id  %></th>
      <td>
        <% if current_user.test_ids.include?(test.id) %>
          <%= link_to "Teste Finalizado", users_backoffice_test_user_test_path(test.id, @user_test), class:'btn btn-info', id:"clicked" %>
        <% else %>
          <%= link_to test.description, new_users_backoffice_test_user_test_path(test.id), class:'btn btn-primary', id:"clicked" %>
        <% end %>
      </td>
      <td>
        <%= test.updated_at %>
      </td>
      <td>
        <%= test.questions.ids.size %>
      </td>
      <td>

      </td>
      </tr>
    <% end %>
  </tbody>
</table>